          <ros>
            <remapping>~/out:=imu</remapping>
          </ros>
        </plugin>
      </sensor>
    </link>


    <joint name='slam_lidar_joint' type='fixed'>
      <parent>base_link</parent>
      <child>slam_lidar</child>
      <pose>0.2 0 2.237 0 0 0</pose>
    </joint>

    <link name='slam_lidar'>
      <inertial>
        <pose>0.2 0 2.237 0 0 0</pose>
        <inertia>
          <ixx>0.001</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.001</iyy>
          <iyz>0.0</iyz>
          <izz>0.002</izz>
        </inertia>
        <mass>0.641</mass>
      </inertial>

      <visual name="slam_lidar_visual">
        <pose>0.2 0 2.237 0 0 0</pose>
        <geometry>
          <mesh>
            <uri>model://agv_common/meshes/C16_3D.stl</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </visual>

      <sensor name="3d_slam_lidar" type="gpu_ray">
        <always_on>true</always_on>
        <visualize>false</visualize>
        <pose>0.2 0 2.219 0 0 0</pose>
        <update_rate>10</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>2000</samples>
              <resolution>1</resolution>
              <min_angle>-3.14</min_angle>
              <max_angle>3.14</max_angle>
            </horizontal>
            <vertical>
              <samples>16</samples>
              <resolution>1</resolution>
              <min_angle>0.2443</min_angle>
              <max_angle>-0.2793</max_angle>
            </vertical>
          </scan>
          <range>
            <min>0.8</min>
            <max>200</max>
            <resolution>0.03</resolution>
          </range>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.001</stddev>
          </noise>
        </ray>

        <plugin name="gazebo_ros_laser_controller" filename="libgazebo_ros_velodyne_laser.so">
          <ros>
            <remapping>~/out:=points2</remapping>
          </ros>
          <frame_name>slam_lidar</frame_name>
          <organize_cloud>false</organize_cloud>
          <min_range>0.8</min_range>
          <max_range>200</max_range>
          <gaussianNoise>0.1</gaussianNoise>
        </plugin>

        <!-- <plugin name="gazebo_ros_ray_sensor" filename="libgazebo_ros_ray_sensor.so">
          <output_type>sensor_msgs/PointCloud2</output_type>
          <frameName>slam_lidar</frameName>
          <ros>
            <remapping>~/out:=points2</remapping>
          </ros>
        </plugin> -->
      </sensor>

    </link>


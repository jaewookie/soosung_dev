    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://agv_description/meshes/BODY.stl" />
      </geometry>
      <material name="orange" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://agv_description/meshes/BODY.stl" />
      </geometry>
    </collision>
  </link>

  <joint name="mast_joint" type="fixed">
    <parent link="base_link" />
    <child link="mast" />
    <origin xyz="0 0.631 1.875" rpy="0 0 0" />
  </joint>

  <link name="mast">
    <inertial>

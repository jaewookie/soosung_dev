    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://agv_description/meshes/MAST_RE.stl" />
      </geometry>
    </collision>
  </link>

  <joint name="front_wheel_l_joint" type="continuous">
    <parent link="base_link" />
    <child link="front_wheel_l" />
    <origin xyz="-0.451 0.86 0.035" rpy="0 0 -1.571" />
    <axis xyz="1 0 0"/>
  </joint>

  <link name="front_wheel_l">
    <inertial>
      <mass value="19.247" />
      <inertia ixx="14.401" ixy="-7.39" ixz="-0.473" iyy="3.965" iyz="0.91" izz="18.142" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://agv_description/meshes/FRONT_WHEEL_RE2.stl"/>

      <geometry>
        <mesh filename="package://agv_description/meshes/FRONT_WHEEL_RE2.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="steering_joint" type="continuous">
    <parent link="base_link" />
    <child link="steering_part" />
    <origin xyz="0.0456 -0.212 0.375" rpy="0 0 0" />
    <axis xyz="0 0 1"/>
  </joint>


  <link name="steering_part">
    <inertial>
      <mass value="85.197" />
      <inertia ixx="2.8" ixy="0" ixz="0.11" iyy="2.84" iyz="-0.11" izz="0.93" />
